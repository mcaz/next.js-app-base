{
  "name": "next.js-ssr-portfolio",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && next start",
    "export": "next build && next export",
    "deploy": "bash ./bin/deploy.sh",
    "lint": "eslint --cache --fix src && prettier --write src",
    "check:format-rule": "eslint-config-prettier 'src/**/*.{js,jsx,ts,tsx}'",
