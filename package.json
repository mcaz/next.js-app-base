    "urix": "^0.1.0",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --check src",
      "eslint src --ext .js,.ts,.tsx"
    ]
  },
  "prepare": "husky install"
}
